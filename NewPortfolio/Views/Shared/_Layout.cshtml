
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="Diablo 4 LazarusWill" />
    <meta name="keywords" content="Diablo 4, LazarusWill, lazarus will" />
    <title>@ViewData["Title"] - LazarusWill</title>
    <link rel="shortcut icon" href="~/images/euro (1).png" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://unpkg.com/boxicons@latest/css/boxicons.min.css">
    <link href="https://cdn.jsdelivr.net/npm/remixicon@3.2.0/fonts/remixicon.css" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Bruno+Ace+SC&family=Pangolin&family=Poppins:ital,wght@0,200;0,500;0,700;1,300;1,500&display=swap" rel="stylesheet">
  

</head>
<body>
    <header>
        <div>
            <a class="logo" href="">LazarusWill<small>.CZ</small></a>
            <i class="ri-youtube-line"></i>
        </div>

        <ul class="navlist">
            <li><a asp-area="" asp-controller="Home" asp-action="Index">Domů</a></li>
            <li><a asp-area="" asp-controller="Articles" asp-action="Index">Články</a></li>   
            <li><a asp-area="" asp-controller="BuildPosts" asp-action="Index">Buildy</a></li>
            <li><a asp-area="" asp-controller="Items" asp-action="Index">Itemy</a></li>
            <li><a asp-area="" asp-controller="Home" asp-action="Contact">Kontakt</a></li>
           
          
            <li>
                <a href="/Account/Administration">
                @if (User.Identity.IsAuthenticated)
                {
                  @*  <a asp-area="" asp-controller="Humen" asp-action="Index">Humans</a>*@
                }

                else
                {
                    <a style="display: none" asp-area="" asp-controller="" asp-action="">Human</a>
                }
                </a>
            </li>
           
            <li>
                <a href="/Account/Administration">
                    @if (User.Identity.IsAuthenticated)
                    {
                        <span >Účet</span>
                    }
                    else
                    {
                        <span>Přihlásit</span>
                    }
                </a>
            </li>
        </ul>
        <div class="bx bx-menu" id="menu-icon"></div>
    </header>

    <main>
        @RenderBody()
    </main>
    <footer>
        <div>
            &copy; @DateTime.Now.Year LazarusWill.cz <a asp-area="" asp-controller="Home" asp-action="RegisterAgree"> Pravidla</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
  
    
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
