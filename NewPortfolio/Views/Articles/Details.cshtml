@model NewPortfolio.Models.Article

@{
    ViewData["Title"] = "Details";
}


@section Scripts
    {
    <script src="https://cdnjs.cloudflare.com/ajax/libs/tinymce/6.0.3/tinymce.min.js"></script>
    <script src="@Url.Content("~/js/initTinyMCE.js")"></script>
}

<section class="section-index">
    <div class="section-hero">


        <p></p>
        <p class="p-index">
            @Html.Raw(Model.Title+" - "+ Model.Description) <br />
            @Html.Raw(Model.Content)
        </p>
        <hr />
        <a class="btn-use btn-detail" asp-action="Index">Zpět na seznam</a>


    </div>
</section>